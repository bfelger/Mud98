{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "WorldEdit",
  "version": "0.1.0",
  "identifier": "com.mud98.worldedit",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "title": "WorldEdit",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": false
  }
}
